<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidprojects.greggy.funmath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartGame" />
        <activity android:name=".Addition" />
        <activity android:name=".Subtraction" />
        <activity android:name=".GameOver" />
        <activity android:name=".Multiplication" />
        <activity android:name=".Highscore" />
        <activity android:name=".Tutorial" />
        <activity android:name=".DeveloperName"></activity>
    </application>

</manifest>